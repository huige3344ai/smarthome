<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">

<struts>
	<constant name="struts.multipart.saveDir" value="/tmp"/>
	<package name="devices" namespace="/" extends="struts">
		<action name="devicesActionb_*" class="devicesAction" method="{1}">
			<result name="direct_devicesList" type="redirect">devicesActionb_devicesList.action</result>
			<result name="{1}">page/background/device/{1}.jsp</result>
		</action>
	
	
	</package>
</struts>    
